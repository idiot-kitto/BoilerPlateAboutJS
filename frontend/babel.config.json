{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets" : "> 0.25%, not dead",
                "useBuiltIns" : "usage",
                "corejs" : { "version": 3, "proposals": true },
                "modules" : false
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties"
        // 클래스 상속시 field가 undefined 나는 에러를 잡는 plugin
    ]
}